<?xml version="1.0"?>
<h:html xmlns:esri="http://esri.com/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms">
  <h:head>
    <h:title>AGEO - GeoHazards report</h:title>
    <model>
      <itext>
        <translation lang="default" default="true()">
          <text id="/AGEO___GeoHazards_report/intro/generated_note_name_7:label">
            <value><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></value>
            <value form="image">jr://images/logoAGEO.png</value>
          </text>
          <text id="/AGEO___GeoHazards_report/intro/generated_note_name_9:label">
            <value>&lt;center&gt;
&lt;font size="0.2" color="dimgrey"&gt;Aplicação disponibilizada por:&lt;/font&gt;
&lt;/center&gt;</value>
            <value form="image">jr://images/logo_CML.png</value>
          </text>
          <text id="/AGEO___GeoHazards_report/geohazard_type/event_type:label">
            <value><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></value>
            <value form="image">jr://images/geohazard_type.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/settlements/building_settlement:label">
            <value>Tipo de assentamento</value>
            <value form="image">jr://images/building_settlement.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/settlements/other_structures:label">
            <value>Outras estruturas</value>
            <value form="image">jr://images/settlement_other_structures.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/settlements/crack_direction:label">
            <value>Direcção da fissuração</value>
            <value form="image">jr://images/crack_buttons.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/landslides/damages:label">
            <value>Danos observados</value>
            <value form="image">jr://images/landslides_damages.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/landslides/volume:label">
            <value>Volume de terras deslocadas</value>
            <value form="image">jr://images/volume_icons.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/floods/floods_damages:label">
            <value>Danos observados</value>
            <value form="image">jr://images/floods_damages.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/floods/water_level:label">
            <value>Nível de água</value>
            <value form="image">jr://images/water_level.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/sinkholes/sinkhole_location:label">
            <value>Local</value>
            <value form="image">jr://images/sinkhole_location.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/sinkholes/damaged_infrastructures:label">
            <value>Infraestruturas afetadas</value>
            <value form="image">jr://images/urban_infrastructures.svg</value>
          </text>
          <text id="/AGEO___GeoHazards_report/damaged_buildings/damage_level:label">
            <value><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></value>
            <value form="image">jr://images/building_damages.svg</value>
          </text>
        </translation>
      </itext>
      <instance>
        <AGEO___GeoHazards_report id="geohazards">
          <generated_note_form_title/>
          <generated_note_calc_title/>
          <generated_note_no_label/>
          <intro>
            <generated_note_name_7/>
            <generated_note_name_8/>
            <generated_note_name_9/>
          </intro>
          <geohazard_type>
            <event_type/>
          </geohazard_type>
          <location_page>
            <event_point/>
            <event_date>now()</event_date>
            <present/>
            <elements_at_risk/>
          </location_page>
          <settlements_count/>
          <settlements jr:template="">
            <building_settlement/>
            <other_structures/>
            <crack_direction/>
            <crack_dim/>
          </settlements>
          <landslides_count/>
          <landslides jr:template="">
            <damages/>
            <damages_other/>
            <volume/>
          </landslides>
          <floods_count/>
          <floods jr:template="">
            <floods_damages/>
            <floods_damages_other/>
            <water_level/>
          </floods>
          <sinkholes_count/>
          <sinkholes jr:template="">
            <sinkhole_dim/>
            <sinkhole_location/>
            <sinkhole_location_other/>
            <damaged_infrastructures/>
            <damaged_infrastructures_other/>
          </sinkholes>
          <damaged_buildings jr:template="">
            <damage_level/>
            <floors>1</floors>
          </damaged_buildings>
          <photos_page>
            <photos jr:template="">
              <foto/>
            </photos>
            <observ/>
          </photos_page>
          <earthquakes>
            <generated_note_name_64/>
          </earthquakes>
          <meta>
            <instanceID/>
          </meta>
        </AGEO___GeoHazards_report>
      </instance>
      <bind nodeset="/AGEO___GeoHazards_report/generated_note_form_title" readonly="true()" type="string"/>
      <bind nodeset="/AGEO___GeoHazards_report/generated_note_calc_title" readonly="true()" type="string" calculate="if( /AGEO___GeoHazards_report/geohazard_type/event_type  != '', '&lt;b&gt;'+jr:choice-name( /AGEO___GeoHazards_report/geohazard_type/event_type , ' /AGEO___GeoHazards_report/geohazard_type/event_type ')+'&lt;/b&gt;', '')"/>
      <bind nodeset="/AGEO___GeoHazards_report/generated_note_no_label" readonly="true()" type="string"/>
      <bind nodeset="/AGEO___GeoHazards_report/intro/generated_note_name_7" readonly="true()" type="string"/>
      <bind nodeset="/AGEO___GeoHazards_report/intro/generated_note_name_8" readonly="true()" type="string"/>
      <bind nodeset="/AGEO___GeoHazards_report/intro/generated_note_name_9" readonly="true()" type="string"/>
      <bind nodeset="/AGEO___GeoHazards_report/geohazard_type/event_type" type="select1" required="true()"/>
      <bind nodeset="/AGEO___GeoHazards_report/location_page" relevant=" /AGEO___GeoHazards_report/geohazard_type/event_type  != ''"/>
      <bind nodeset="/AGEO___GeoHazards_report/location_page/event_point" type="geopoint" esri:warning_message="Localização GPS pouco precisa!" esri:warning="pulldata(&quot;@geopoint&quot;, ., &quot;horizontalAccuracy&quot;) &lt; 5"/>
      <bind nodeset="/AGEO___GeoHazards_report/location_page/event_date" type="date"/>
      <bind nodeset="/AGEO___GeoHazards_report/location_page/present" type="select" esri:fieldType="esriFieldTypeSingle"/>
      <bind nodeset="/AGEO___GeoHazards_report/location_page/elements_at_risk" type="select"/>
      <bind nodeset="/AGEO___GeoHazards_report/settlements_count" type="string" readonly="true()" calculate="1"/>
      <bind nodeset="/AGEO___GeoHazards_report/settlements" relevant=" /AGEO___GeoHazards_report/geohazard_type/event_type  = 'settlement'"/>
      <bind nodeset="/AGEO___GeoHazards_report/settlements/building_settlement" type="select1"/>
      <bind nodeset="/AGEO___GeoHazards_report/settlements/other_structures" type="select1"/>
      <bind nodeset="/AGEO___GeoHazards_report/settlements/crack_direction" type="select"/>
      <bind nodeset="/AGEO___GeoHazards_report/settlements/crack_dim" type="select1" esri:fieldType="esriFieldTypeSingle"/>
      <bind nodeset="/AGEO___GeoHazards_report/landslides_count" type="string" readonly="true()" calculate="1"/>
      <bind nodeset="/AGEO___GeoHazards_report/landslides" relevant=" /AGEO___GeoHazards_report/geohazard_type/event_type  = 'landslide'"/>
      <bind nodeset="/AGEO___GeoHazards_report/landslides/damages" type="select"/>
      <bind nodeset="/AGEO___GeoHazards_report/landslides/damages_other" type="string" relevant="contains( /AGEO___GeoHazards_report/landslides/damages ,'other')"/>
      <bind nodeset="/AGEO___GeoHazards_report/landslides/volume" type="select1" esri:fieldType="esriFieldTypeSingle"/>
      <bind nodeset="/AGEO___GeoHazards_report/floods_count" type="string" readonly="true()" calculate="1"/>
      <bind nodeset="/AGEO___GeoHazards_report/floods" relevant=" /AGEO___GeoHazards_report/geohazard_type/event_type  = 'flood'"/>
      <bind nodeset="/AGEO___GeoHazards_report/floods/floods_damages" type="select"/>
      <bind nodeset="/AGEO___GeoHazards_report/floods/floods_damages_other" type="string" relevant="contains( /AGEO___GeoHazards_report/floods/floods_damages ,'other')"/>
      <bind nodeset="/AGEO___GeoHazards_report/floods/water_level" type="select1" esri:fieldType="esriFieldTypeSingle"/>
      <bind nodeset="/AGEO___GeoHazards_report/sinkholes_count" type="string" readonly="true()" calculate="1"/>
      <bind nodeset="/AGEO___GeoHazards_report/sinkholes" relevant=" /AGEO___GeoHazards_report/geohazard_type/event_type  = 'sinkhole'"/>
      <bind nodeset="/AGEO___GeoHazards_report/sinkholes/sinkhole_dim" type="select1" esri:fieldType="esriFieldTypeSingle"/>
      <bind nodeset="/AGEO___GeoHazards_report/sinkholes/sinkhole_location" type="select1"/>
      <bind nodeset="/AGEO___GeoHazards_report/sinkholes/sinkhole_location_other" type="string" relevant="contains( /AGEO___GeoHazards_report/sinkholes/sinkhole_location ,'other')"/>
      <bind nodeset="/AGEO___GeoHazards_report/sinkholes/damaged_infrastructures" type="select"/>
      <bind nodeset="/AGEO___GeoHazards_report/sinkholes/damaged_infrastructures_other" type="string" relevant="contains( /AGEO___GeoHazards_report/sinkholes/damaged_infrastructures ,'other')"/>
      <bind nodeset="/AGEO___GeoHazards_report/damaged_buildings" relevant=" /AGEO___GeoHazards_report/geohazard_type/event_type  = 'earthquake'"/>
      <bind nodeset="/AGEO___GeoHazards_report/damaged_buildings/damage_level" type="select1" required="true()"/>
      <bind nodeset="/AGEO___GeoHazards_report/damaged_buildings/floors" type="int" esri:fieldType="esriFieldTypeSingle"/>
      <bind nodeset="/AGEO___GeoHazards_report/photos_page" relevant=" /AGEO___GeoHazards_report/geohazard_type/event_type  != ''"/>
      <bind nodeset="/AGEO___GeoHazards_report/photos_page/photos/foto" type="binary"/>
      <bind nodeset="/AGEO___GeoHazards_report/photos_page/observ" type="string"/>
      <bind nodeset="/AGEO___GeoHazards_report/earthquakes" relevant=" /AGEO___GeoHazards_report/geohazard_type/event_type  = 'earthquake'"/>
      <bind nodeset="/AGEO___GeoHazards_report/earthquakes/generated_note_name_64" readonly="true()" type="string"/>
      <bind nodeset="/AGEO___GeoHazards_report/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <input ref="/AGEO___GeoHazards_report/generated_note_form_title">
      <label><output value=" /AGEO___GeoHazards_report/generated_note_calc_title "/></label>
    </input>
    <input appearance="hidden" ref="/AGEO___GeoHazards_report/generated_note_calc_title">
      <label>no label</label>
    </input>
    <input appearance="hidden" ref="/AGEO___GeoHazards_report/generated_note_no_label">
      <label>no label</label>
    </input>
    <group appearance="field-list" ref="/AGEO___GeoHazards_report/intro">
      <label><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></label>
      <input ref="/AGEO___GeoHazards_report/intro/generated_note_name_7">
        <label ref="jr:itext('/AGEO___GeoHazards_report/intro/generated_note_name_7:label')"/>
      </input>
      <input ref="/AGEO___GeoHazards_report/intro/generated_note_name_8">
        <label>&lt;center&gt;
Aplicação para reporte de eventos relacionados com fenómenos naturais.
&lt;/center&gt;</label>
      </input>
      <input ref="/AGEO___GeoHazards_report/intro/generated_note_name_9">
        <label ref="jr:itext('/AGEO___GeoHazards_report/intro/generated_note_name_9:label')"/>
        <hint>&lt;center&gt;&lt;a href="https://www.lisboa.pt/politica-de-privacidade"&gt;&lt;font size="0.1" color="grey"&gt;Política de privacidade&lt;/font&gt;&lt;/a&gt;&lt;/center&gt;</hint>
      </input>
    </group>
    <group appearance="field-list" ref="/AGEO___GeoHazards_report/geohazard_type">
      <label>Selecione o tipo de ocorrência</label>
      <select1 appearance="image-map" ref="/AGEO___GeoHazards_report/geohazard_type/event_type">
        <label ref="jr:itext('/AGEO___GeoHazards_report/geohazard_type/event_type:label')"/>
        <item>
          <label>Sismo</label>
          <value>earthquake</value>
        </item>
        <item>
          <label>Assentamento</label>
          <value>settlement</value>
        </item>
        <item>
          <label>Abatimento</label>
          <value>sinkhole</value>
        </item>
        <item>
          <label>Queda de blocos</label>
          <value>rockfall</value>
        </item>
        <item>
          <label>Inundação</label>
          <value>flood</value>
        </item>
        <item>
          <label>Deslizamento</label>
          <value>landslide</value>
        </item>
        <item>
          <label>Tsunami</label>
          <value>tsunami</value>
        </item>
      </select1>
    </group>
    <group appearance="field-list" ref="/AGEO___GeoHazards_report/location_page">
      <label>Informação sobre o evento</label>
      <input ref="/AGEO___GeoHazards_report/location_page/event_point">
        <label>&lt;b&gt;Localização&lt;/b&gt;</label>
      </input>
      <input ref="/AGEO___GeoHazards_report/location_page/event_date">
        <label>Data do evento</label>
      </input>
      <select ref="/AGEO___GeoHazards_report/location_page/present">
        <label><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></label>
        <item>
          <label>Presenciou o evento nesta data</label>
          <value>1</value>
        </item>
      </select>
      <select ref="/AGEO___GeoHazards_report/location_page/elements_at_risk">
        <label>Elementos em risco</label>
        <item>
          <label>Pessoas</label>
          <value>people</value>
        </item>
        <item>
          <label>Animais</label>
          <value>animals</value>
        </item>
        <item>
          <label>Bens</label>
          <value>assets</value>
        </item>
      </select>
    </group>
    <group ref="/AGEO___GeoHazards_report/settlements">
      <label><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></label>
      <repeat nodeset="/AGEO___GeoHazards_report/settlements" appearance="field-list" jr:count=" /AGEO___GeoHazards_report/settlements_count ">
        <select1 appearance="image-map" ref="/AGEO___GeoHazards_report/settlements/building_settlement">
          <label ref="jr:itext('/AGEO___GeoHazards_report/settlements/building_settlement:label')"/>
          <item>
            <label>1</label>
            <value>1</value>
          </item>
          <item>
            <label>2</label>
            <value>2</value>
          </item>
          <item>
            <label>3</label>
            <value>3</value>
          </item>
          <item>
            <label>4</label>
            <value>4</value>
          </item>
        </select1>
        <select1 appearance="image-map" ref="/AGEO___GeoHazards_report/settlements/other_structures">
          <label ref="jr:itext('/AGEO___GeoHazards_report/settlements/other_structures:label')"/>
          <item>
            <label>Muro</label>
            <value>wall</value>
          </item>
          <item>
            <label>Escadaria</label>
            <value>stairs</value>
          </item>
        </select1>
        <select appearance="image-map" ref="/AGEO___GeoHazards_report/settlements/crack_direction">
          <label ref="jr:itext('/AGEO___GeoHazards_report/settlements/crack_direction:label')"/>
          <item>
            <label>Vertical</label>
            <value>vertical</value>
          </item>
          <item>
            <label>Horizontal</label>
            <value>horizontal</value>
          </item>
          <item>
            <label>Diagonal (direita)</label>
            <value>diagonal_right</value>
          </item>
          <item>
            <label>Diagonal (esquerda)</label>
            <value>diagonal_left</value>
          </item>
          <item>
            <label>Estrelada</label>
            <value>star</value>
          </item>
          <item>
            <label>Sem fissuras</label>
            <value>none</value>
          </item>
        </select>
        <select1 appearance="likert" ref="/AGEO___GeoHazards_report/settlements/crack_dim">
          <label>Dimensão das fissuras</label>
          <item>
            <label>&lt; 0,3 mm</label>
            <value>1</value>
          </item>
          <item>
            <label>&lt; 1,5 mm</label>
            <value>2</value>
          </item>
          <item>
            <label>&lt; 5 mm</label>
            <value>3</value>
          </item>
          <item>
            <label>&lt; 15 mm</label>
            <value>4</value>
          </item>
          <item>
            <label>&gt; 15 mm</label>
            <value>5</value>
          </item>
        </select1>
      </repeat>
    </group>
    <group ref="/AGEO___GeoHazards_report/landslides">
      <label><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></label>
      <repeat nodeset="/AGEO___GeoHazards_report/landslides" appearance="field-list" jr:count=" /AGEO___GeoHazards_report/landslides_count ">
        <select appearance="image-map" ref="/AGEO___GeoHazards_report/landslides/damages">
          <label ref="jr:itext('/AGEO___GeoHazards_report/landslides/damages:label')"/>
          <item>
            <label>Edifícios</label>
            <value>buildings</value>
          </item>
          <item>
            <label>Zona verde</label>
            <value>green_area</value>
          </item>
          <item>
            <label>Estrada</label>
            <value>road</value>
          </item>
          <item>
            <label>Ferrovia</label>
            <value>railroad</value>
          </item>
          <item>
            <label>Linhas de água</label>
            <value>waterlines</value>
          </item>
          <item>
            <label>Outros</label>
            <value>other</value>
          </item>
        </select>
        <input appearance="multiline" esri:style="defaultHeight=2" ref="/AGEO___GeoHazards_report/landslides/damages_other">
          <label>Outros danos</label>
        </input>
        <select1 appearance="image-map" ref="/AGEO___GeoHazards_report/landslides/volume">
          <label ref="jr:itext('/AGEO___GeoHazards_report/landslides/volume:label')"/>
          <hint>&lt;i&gt;(Volume equivalente a)&lt;/i&gt;</hint>
          <item>
            <label>Carro</label>
            <value>1</value>
          </item>
          <item>
            <label>Carrinha</label>
            <value>2</value>
          </item>
          <item>
            <label>Autocarro</label>
            <value>3</value>
          </item>
          <item>
            <label>Camião TIR</label>
            <value>4</value>
          </item>
          <item>
            <label>&gt; Camião TIR</label>
            <value>5</value>
          </item>
        </select1>
      </repeat>
    </group>
    <group ref="/AGEO___GeoHazards_report/floods">
      <label><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></label>
      <repeat nodeset="/AGEO___GeoHazards_report/floods" appearance="field-list" jr:count=" /AGEO___GeoHazards_report/floods_count ">
        <select appearance="image-map" ref="/AGEO___GeoHazards_report/floods/floods_damages">
          <label ref="jr:itext('/AGEO___GeoHazards_report/floods/floods_damages:label')"/>
          <item>
            <label>Edifícios</label>
            <value>buildings</value>
          </item>
          <item>
            <label>Zona verde</label>
            <value>green_area</value>
          </item>
          <item>
            <label>Estrada</label>
            <value>road</value>
          </item>
          <item>
            <label>Ferrovia</label>
            <value>railroad</value>
          </item>
          <item>
            <label>Galgamento de margens</label>
            <value>banks_overflow</value>
          </item>
          <item>
            <label>Outros</label>
            <value>other</value>
          </item>
        </select>
        <input appearance="multiline" ref="/AGEO___GeoHazards_report/floods/floods_damages_other">
          <label>Outros danos</label>
        </input>
        <select1 appearance="image-map" ref="/AGEO___GeoHazards_report/floods/water_level">
          <label ref="jr:itext('/AGEO___GeoHazards_report/floods/water_level:label')"/>
          <item>
            <label>1</label>
            <value>1</value>
          </item>
          <item>
            <label>2</label>
            <value>2</value>
          </item>
          <item>
            <label>3</label>
            <value>3</value>
          </item>
          <item>
            <label>4</label>
            <value>4</value>
          </item>
          <item>
            <label>5</label>
            <value>5</value>
          </item>
          <item>
            <label>6</label>
            <value>6</value>
          </item>
        </select1>
      </repeat>
    </group>
    <group ref="/AGEO___GeoHazards_report/sinkholes">
      <label><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></label>
      <repeat nodeset="/AGEO___GeoHazards_report/sinkholes" appearance="field-list" jr:count=" /AGEO___GeoHazards_report/sinkholes_count ">
        <select1 appearance="likert" ref="/AGEO___GeoHazards_report/sinkholes/sinkhole_dim">
          <label>&lt;p&gt;Dimensão (em passos)&lt;p/&gt;&lt;img src="steps.png" /&gt;</label>
          <item>
            <label>1</label>
            <value>1</value>
          </item>
          <item>
            <label>2</label>
            <value>2</value>
          </item>
          <item>
            <label>3</label>
            <value>3</value>
          </item>
          <item>
            <label>4</label>
            <value>4</value>
          </item>
          <item>
            <label>5</label>
            <value>5</value>
          </item>
          <item>
            <label>≥ 6</label>
            <value>6</value>
          </item>
        </select1>
        <select1 appearance="image-map" ref="/AGEO___GeoHazards_report/sinkholes/sinkhole_location">
          <label ref="jr:itext('/AGEO___GeoHazards_report/sinkholes/sinkhole_location:label')"/>
          <item>
            <label>Rodovia</label>
            <value>road</value>
          </item>
          <item>
            <label>Zona pedonal</label>
            <value>pedestrian_area</value>
          </item>
