      <label>no label</label>
    </input>
    <input appearance="hidden" ref="/AGEO___GeoHazards_report/generated_note_no_label">
      <label>no label</label>
    </input>
    <group appearance="field-list" ref="/AGEO___GeoHazards_report/intro">
      <label><output value=" /AGEO___GeoHazards_report/generated_note_no_label "/></label>
      <input ref="/AGEO___GeoHazards_report/intro/generated_note_name_7">
        <label>&lt;div style="line-height: 50px"&gt; &lt;/div&gt;</label>
      </input>
      <input ref="/AGEO___GeoHazards_report/intro/generated_note_name_8">
        <label ref="jr:itext('/AGEO___GeoHazards_report/intro/generated_note_name_8:label')"/>
      </input>
      <input ref="/AGEO___GeoHazards_report/intro/generated_note_name_9">
        <label ref="jr:itext('/AGEO___GeoHazards_report/intro/generated_note_name_9:label')"/>
      </input>
      <input ref="/AGEO___GeoHazards_report/intro/generated_note_name_10">
        <label>&lt;center&gt;&lt;font size="3" style="font-family: Calibri, fantasy; font-weight: bold"&gt;
Aplicação para reporte de eventos relacionados com fenómenos naturais.
&lt;/font&gt;&lt;/center&gt;</label>
      </input>
      <input ref="/AGEO___GeoHazards_report/intro/generated_note_name_11">
        <label ref="jr:itext('/AGEO___GeoHazards_report/intro/generated_note_name_11:label')"/>
      </input>
      <input ref="/AGEO___GeoHazards_report/intro/generated_note_name_12">
        <label ref="jr:itext('/AGEO___GeoHazards_report/intro/generated_note_name_12:label')"/>
        <hint>&lt;center&gt;&lt;a href="https://www.lisboa.pt/politica-de-privacidade"&gt;&lt;font size="0.1" color="grey"&gt;Política de privacidade&lt;/font&gt;&lt;/a&gt;&lt;/center&gt;</hint>
      </input>
    </group>
    <group appearance="field-list" ref="/AGEO___GeoHazards_report/geohazard_type">
      <label>&lt;center&gt;Selecione o tipo de ocorrência&lt;/center&gt;</label>
      <select1 appearance="image-map" ref="/AGEO___GeoHazards_report/geohazard_type/event_type">
        <label ref="jr:itext('/AGEO___GeoHazards_report/geohazard_type/event_type:label')"/>
        <item>
          <label>Sismo</label>
          <value>earthquake</value>
        </item>
        <item>
          <label>Assentamento</label>
          <value>settlement</value>
        </item>
        <item>
          <label>Abatimento</label>
          <value>sinkhole</value>
        </item>
        <item>
          <label>Queda de blocos</label>
          <value>rockfall</value>
        </item>
        <item>
          <label>Inundação</label>
          <value>flood</value>
        </item>
        <item>
          <label>Deslizamento</label>
          <value>landslide</value>
        </item>
        <item>
          <label>Tsunami</label>
          <value>tsunami</value>
        </item>
        <item>
          <label>Outro</label>
          <value>other</value>
        </item>
      </select1>
      <input ref="/AGEO___GeoHazards_report/geohazard_type/generated_note_name_17">
        <label>&lt;center&gt;&lt;i&gt;Para reporte de sismos ou réplicas e de danos associados&lt;/i&gt;&lt;/center&gt;</label>
      </input>
      <input ref="/AGEO___GeoHazards_report/geohazard_type/generated_note_name_18">
        <label>&lt;center&gt;&lt;i&gt;Para reporte de danos em estruturas edificadas que possam evidenciar a ocorrência de assentamento dos solos de base (ex.: fissuras, fendilhamentos, desagregação de materiais, etc)&lt;/i&gt;&lt;/center&gt;</label>
      </input>
      <input ref="/AGEO___GeoHazards_report/geohazard_type/generated_note_name_19">
        <label>&lt;center&gt;&lt;i&gt;Para reporte de movimentos de grandes massas de solos / deslizamentos de terras e queda de muros de suporte&lt;/i&gt;&lt;/center&gt;</label>
